{
  "compilerOptions": {
    "resolveJsonModule": true,
    "types": [
      "node", "jest"
    ],
    "allowJs": true,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "target": "es6",
    "strict": false,
    "noEmit": true,
    "module": "esnext",
    "esModuleInterop": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "noImplicitAny": false,
    "lib": [
      "dom",
      "es2017"
    ],
    "baseUrl": "./",
    "paths": {
      "@@/*": ["src/.umi/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}